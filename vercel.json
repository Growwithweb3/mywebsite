{
  "rewrites": [
    {
      "source": "/Basic-of-web-3-part-1.pdf",
      "destination": "/public/Basic-of-web-3-part-1.pdf"
    },
    {
      "source": "/Basic-of-web-3-part-2.pdf",
      "destination": "/public/Basic-of-web-3-part-2.pdf"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.pdf)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/pdf"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

